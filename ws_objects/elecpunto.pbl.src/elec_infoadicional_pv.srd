         CLIENTE.TELEFONO,   
         '   'direccion1,
         CLIENTE.DIREC_ELECTRONICA  
    FROM pv_nota_venta,
         CLIENTE  
   WHERE ( CLIENTE.COD_CLIENTE = pv_nota_venta.COD_CLIENTE ) and  
          ( pv_nota_venta.tipo_doc = :ls_tipo ) AND  
         ( pv_nota_venta.numero = :ll_doc ) and
