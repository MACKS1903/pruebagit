        FROM compania ,
           pv_det_nota_venta ,
           pv_nota_venta ,
           inv_articulo ,
           unidad ,
           pv_nv_movimiento ,
           cliente,
           funcionario     
        WHERE ( compania.cod_compania = pv_det_nota_venta.cod_compania ) and
			( pv_nota_venta.cod_cliente = cliente.cod_cliente ) and
          ( pv_nota_venta.cod_compania = pv_det_nota_venta.cod_compania ) and          ( pv_det_nota_venta.numero = pv_nota_venta.numero ) and          ( pv_det_nota_venta.cod_articulo = inv_articulo.cod_articulo ) and          ( pv_det_nota_venta.cod_unidad = unidad.cod_unidad ) and          ( pv_nv_movimiento.cod_compania = pv_nota_venta.cod_compania ) and          ( pv_nota_venta.numero = pv_nv_movimiento.numero ) and          ( funcionario.cod_funcionario = pv_nota_venta.cod_vendedor ) and          ( ( compania.cod_compania = :gs_comp ) and          ( pv_det_nota_venta.numero = :nro ) and          ( pv_nota_venta.nr_caja = :caja ) )   ARG(NAME = "gs_comp" TYPE = string)  ARG(NAME = "nro" TYPE = number)  ARG(NAME = "caja" TYPE = number)