           pv_nota_venta.c_i_o_ruc ,
           pv_nota_venta.total ,
           compania.telefono ,
           pv_nota_venta.fecha_emision ,
		    (pv_nota_venta.dsto_con_iva +pv_nota_venta.dsto_sin_iva) tot_desc , 
			 (pv_det_nota_venta.pvp * pv_det_nota_venta.cant_pres) neto,  
           pv_det_nota_venta.tipo ,
           pv_nota_venta.cambio ,
